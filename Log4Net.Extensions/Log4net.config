      <appender-ref ref="FileRequestAppender" />
    </logger>
    <logger name="SqlLogger" additivity="false">
      <level value="INFO"/>
      <appender-ref ref="FileSqlAppender" />
    </logger>
    <logger name="Logger" additivity="false">
