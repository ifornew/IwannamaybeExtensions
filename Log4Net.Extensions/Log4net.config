        <conversionPattern value="[%date]%level:%message%n[thread]%thread%n[%logger]:%property{NDC}%n[context]%property{values}%n[stacktrace]%n%exception%newline"/>
      </layout>
    </appender>
    
    <logger name="RequestLogger" additivity="false">
